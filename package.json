{
  "name": "p2pvps-server2",
  "version": "1.0.0",
  "description": "Second incarnation of P2P VPS server. More info at http://P2PVPS.org",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "./node_modules/.bin/nodemon index.js",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha -t 10000",
    "lint": "eslint src/**/*.js",
    "docs": "./node_modules/.bin/apidoc -i src/ -o docs",
    "test-travis": "NODE_ENV=test ./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- -R spec -t 10000 ./test/a*.js"
  },
  "keywords": [
    "koa2-api-boilerplate",
    "api",
    "koa",
    "koa2",
    "boilerplate",
    "es6",
    "mongoose",
    "passportjs",
    "apidoc",
    "p2pvps",
    "vps",
    "javascript",
    "censorship",
    "server"
  ],
  "author": "Chris Troutner <chris.troutner@gmail.com>",
  "license": "MIT",
  "apidoc": {
    "title": "p2pvps-server2",
    "url": "localhost:5000"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/P2PVPS/p2pvps-server2"
  },
  "dependencies": {
    "apidoc": "^0.16.1",
    "bcrypt": "^0.8.5",
    "glob": "^7.0.0",
    "jsonwebtoken": "^7.1.9",
    "kcors": "^2.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-convert": "^1.2.0",
    "koa-generic-session": "^2.0.1",
    "koa-logger": "^3.1.0",
    "koa-mount": "^3.0.0",
    "koa-passport": "^4.0.1",
    "koa-router": "^7.0.1",
    "koa-static": "^4.0.2",
    "mongoose": "^5.0.8",
    "openbazaar-node": "^1.3.1",
    "passport-local": "^1.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.2",
    "require": "^2.4.20"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^3.4.0",
    "eslint-config-standard": "^6.0.0",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-standard": "^2.0.0",
    "istanbul": "^1.1.0-alpha.1",
